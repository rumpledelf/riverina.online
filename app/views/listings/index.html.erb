<p style="color: green"><%= notice %></p>

<% content_for :title, "Listings" %>

<h1><img src="/icon.svg">Welcome to Riverina Online!</h1>

<div id="listings">
  <% @listings.each do |listing| %>
    <%= render listing %>
    <p>
      <%= link_to "Show this listing", listing %>
    </p>
  <% end %>
</div>

<%= link_to "New listing", new_listing_path %>
